<Template xsi:noNamespaceSchemaLocation="Schema.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">  <General>    <TemplateName>Tensile Tests example</TemplateName>    <Description>      Test Type: Tensile      Units : Imperial      File extensions: '.rlt' and '.txt'    </Description>    <FullName>      <Item>Specimen ID</Item>    </FullName>  </General>  <Files>    <TextFile name="FileCode0">      <Extension>txt</Extension>      <Validate>        <Attribute>File Units</Attribute>        <Value>US Customary</Value>      </Validate>      <SpecimenDelimiter>Specimen:[#/%e0-9 \.\-]{1,}</SpecimenDelimiter>    </TextFile>    <TextFile name="FileCode1">      <Extension>rlt</Extension>    </TextFile>  </Files>  <Validation>    <Version>2.9</Version>    <ValidTableNames>      <TableName>Tensile Test Data</TableName>    </ValidTableNames>  </Validation>  <Automation>    <AutoPlacement>      <DBKey>MI_Training</DBKey>      <TableName>Tensile Test Data</TableName>      <MatchAttribute>Specimen ID</MatchAttribute>      <SearchAttribute>Specimen ID</SearchAttribute>      <ForceToRoot>true</ForceToRoot>      <IsChild>false</IsChild>      <ConflictResolution>ReplaceData</ConflictResolution>    </AutoPlacement>    <TreeHierarchy>      <TreeLevel>Titanium</TreeLevel>      <TreeLevel>Alloy</TreeLevel>      <TreeLevel>Plate</TreeLevel>      <TreeLevel>TreeTemp</TreeLevel>    </TreeHierarchy>  </Automation>  <Data>    <TextReaders>      <RegularExpression name="Alloy">        <FileCode>FileCode0</FileCode>        <Expression>\[MATERIAL:\s*\]\[(?&lt;ITEM1&gt;[^\s]{1,}).{1,}\]</Expression>      </RegularExpression>      <RegularExpression name="Material ID">        <FileCode>FileCode0</FileCode>        <Expression>\[MATERIAL CODE:\s*\]\[(?&lt;ITEM1&gt;.{1,})\]</Expression>      </RegularExpression>      <RegularExpression name="Build ID">        <FileCode>FileCode0</FileCode>        <Expression>\[Build ID:\s*\]\[(?&lt;ITEM1&gt;.{1,})\]</Expression>      </RegularExpression>      <RegularExpression name="Test Frame Reference">        <FileCode>FileCode0</FileCode>        <Expression>\[Test Cell:\s*\]\[(?&lt;ITEM1&gt;[[^\]]{1,})\]</Expression>      </RegularExpression>      <RegularExpression name="Project Code">        <FileCode>FileCode0</FileCode>        <Expression>\[Work\#\s*\]\[(?&lt;ITEM1&gt;[^\]]{1,})\]</Expression>      </RegularExpression>      <RegularExpression name="Strain Measurement Reference">        <FileCode>FileCode0</FileCode>        <Expression>\[Extensometer:\s*\]\[(?&lt;ITEM1&gt;[#/@%0-9a-z\^ '\.\-]{1,})</Expression>      </RegularExpression>      <RegularExpression name="Batch Number">        <FileCode>FileCode0</FileCode>        <Expression>\[Batch:\s*\]\[(?&lt;ITEM1&gt;[#/@%0-9a-z\^ '\.\-]{1,})</Expression>      </RegularExpression>      <RegularExpression name="File Units">        <FileCode>FileCode0</FileCode>        <Expression>Units:\s*(?&lt;ITEM1&gt;[#/@%0-9a-z\^ '\.\-]{1,})\s*Geometry</Expression>      </RegularExpression>      <RegularExpression name="Testing series ID" import="true">        <FileCode>FileCode0</FileCode>        <Expression>Series ID:\s*(?&lt;ITEM1&gt;[#/@%0-9a-z\^ '\.\-]{1,})\s*</Expression>      </RegularExpression>      <RegularExpression name="Test Type">        <FileCode>FileCode0</FileCode>        <Expression>TEST TYPE:(?&lt;ITEM1&gt;[#/@%0-9a-z\^ '\.\-]{1,})[ ][ ]</Expression>      </RegularExpression>      <RegularExpression name="Specimen ID">        <FileCode>FileCode0</FileCode>        <Expression>SPECIMEN LABEL:\s*\[(?&lt;ITEM1&gt;.{1,})\]</Expression>        <Multiple>true</Multiple>      </RegularExpression>      <RegularExpression name="Strain Transducer 1 Length">        <FileCode>FileCode0</FileCode>        <Expression>EXT\. GAUGE LEN:\s*(?&lt;ITEM1&gt;[#/%e0-9\.\-]{1,})</Expression>        <Multiple>true</Multiple>        <Unit>in</Unit>      </RegularExpression>      <RegularExpression name="Gauge Length">        <FileCode>FileCode0</FileCode>        <Expression>Spec gauge len:\s*(?&lt;ITEM1&gt;[#/%e0-9 \.\-]{1,})</Expression>        <Multiple>true</Multiple>        <Unit>in</Unit>      </RegularExpression>      <RegularExpression name="Gauge Thickness">        <FileCode>FileCode0</FileCode>        <Expression>Thickness:\s*(?&lt;ITEM1&gt;[#/%e0-9 \.\-]{1,})</Expression>        <Multiple>true</Multiple>        <Unit>in</Unit>      </RegularExpression>      <RegularExpression name="Gauge Width">        <FileCode>FileCode0</FileCode>        <Expression>Width:\s*(?&lt;ITEM1&gt;[#/%e0-9 \.\-]{1,})</Expression>        <Multiple>true</Multiple>        <Unit>in</Unit>      </RegularExpression>      <RegularExpression name="Gauge Outer Diameter">        <FileCode>FileCode0</FileCode>        <Expression>Diameter:\s*(?&lt;ITEM1&gt;[#/%e0-9 \.\-]{1,})</Expression>        <Multiple>true</Multiple>        <Unit>in</Unit>      </RegularExpression>      <RegularExpression name="Operator">        <FileCode>FileCode0</FileCode>        <Expression>OPERATOR NAME:(?&lt;ITEM1&gt;[#/@%0-9a-z\^ '\.\-]{1,})[ ][ ]</Expression>      </RegularExpression>      <RegularExpression name="Test Temperature">        <FileCode>FileCode1</FileCode>        <Expression>"TEMPERATURE \(DEG F\)",(?&lt;ITEM1&gt;[#/%e0-9 \.\-]{1,})</Expression>        <Unit>°F</Unit>      </RegularExpression>      <RegularExpression name="Test Humidity">        <FileCode>FileCode0</FileCode>        <Expression>HUMIDITY:(?&lt;ITEM1&gt;[#/e0-9 \.\-]{1,})</Expression>        <Unit>%</Unit>      </RegularExpression>      <RegularExpression name="Crosshead Speed">        <FileCode>FileCode0</FileCode>        <Expression>RAMP RATE:(?&lt;ITEM1&gt;\-?[#/%e0-9 \.\-]{1,})[#/@%a-z\^ '\.]{1,}\r</Expression>        <Expression>CROSSHEAD SPEED:(?&lt;ITEM1&gt;\-?[#/%e0-9 \.\-]{1,})[#/@%a-z\^ '\.]{1,}\r</Expression>        <Unit>in/min</Unit>      </RegularExpression>      <RegularExpression name="Tensile Comments">        <FileCode>FileCode1</FileCode>        <Expression>"SAMPLE COMMENTS","(?&lt;ITEM1&gt;.*)"</Expression>      </RegularExpression>      <RegularExpression name="Displacement">        <FileCode>FileCode0</FileCode>        <Expression>(\s*([#/%e0-9 \.\-]{1,}),\s*(?&lt;ITEM2&gt;[#/%e0-9 \.\-]{1,}),\s*([#/%e0-9 \.\-]{1,})\r){1,}</Expression>        <Multiple>true</Multiple>        <Unit>in</Unit>      </RegularExpression>      <RegularExpression name="Test Load">        <FileCode>FileCode0</FileCode>        <Expression>(\s*([#/%e0-9 \.\-]{1,}),\s*([#/%e0-9 \.\-]{1,}),\s*(?&lt;ITEM3&gt;[#/%e0-9 \.\-]{1,})\r){1,}</Expression>        <Multiple>true</Multiple>        <Unit>lbf</Unit>      </RegularExpression>      <RegularExpression name="Test Data Index">        <FileCode>FileCode0</FileCode>        <Expression>(\s*(?&lt;ITEM1&gt;[#/%e0-9 \.\-]{1,}),\s*([#/%e0-9 \.\-]{1,}),\s*([#/%e0-9 \.\-]{1,})\r){1,}</Expression>        <Multiple>true</Multiple>      </RegularExpression>      <RegularExpression name="UDF Index">        <FileCode>FileCode0</FileCode>        <Expression>USER DEFINED FIELDS:\r(\s*(?&lt;ITEM1&gt;[#/%e0-9 \.\-]{1,}):\[([#/@%0-9a-z\^ '\.\-]{1,}).*\]\[([#/@%0-9a-z\^ '\.\\":\-]{1,}).*\]\r){1,}</Expression>      </RegularExpression>      <RegularExpression name="UDF Label">        <FileCode>FileCode0</FileCode>        <Expression>USER DEFINED FIELDS:\r(\s*([#/%e0-9 \.\-]{1,}):\[(?&lt;ITEM2&gt;[#/@%0-9a-z\^ '\.\-]{1,}).*\]\[([#/@%0-9a-z\^ '\.\\":\-]{1,}).*\]\r){1,}</Expression>      </RegularExpression>      <RegularExpression name="UDF Comment">        <FileCode>FileCode0</FileCode>        <Expression>USER DEFINED FIELDS:\r(\s*([#/%e0-9 \.\-]{1,}):\[([#/@%0-9a-z\^ '\.\-]{1,}).*\]\[(?&lt;ITEM3&gt;[#/@%0-9a-z\^ '\.\\":\-]{1,}).*\]\r){1,}</Expression>      </RegularExpression>      <RegularExpression name="Index">        <FileCode>FileCode0</FileCode>        <Expression>AUXILIARY SAMPLE INPUTS:\r(\s*(?&lt;ITEM1&gt;[#/%e0-9 \.\-]{1,}):\s*([#/%e0-9 \.\-]{1,})\s*\r){1,}</Expression>      </RegularExpression>      <RegularExpression name="Input">        <FileCode>FileCode0</FileCode>        <Expression>AUXILIARY SAMPLE INPUTS:\r(\s*([#/%e0-9 \.\-]{1,}):\s*(?&lt;ITEM2&gt;[#/%e0-9 \.\-]{1,})\s*\r){1,}</Expression>      </RegularExpression>      <RegularExpression name="Sample Index">        <FileCode>FileCode0</FileCode>        <Expression>AUXILIARY SPECIMEN INPUTS:\s*\r(\s*(?&lt;ITEM1&gt;[#/%e0-9 \.\-]{1,}):\s*([#/%e0-9 \.\-]{1,})\s*\r){1,}</Expression>        <Multiple>true</Multiple>      </RegularExpression>      <RegularExpression name="Sample Input">        <FileCode>FileCode0</FileCode>        <Expression>AUXILIARY SPECIMEN INPUTS:\s*\r(\s*([#/%e0-9 \.\-]{1,}):\s*(?&lt;ITEM2&gt;[#/%e0-9 \.\-]{1,})\s*\r){1,}</Expression>        <Multiple>true</Multiple>      </RegularExpression>      <Table name="Summary One">        <FileCode>FileCode1</FileCode>        <Header>          <Expression>\r\s*"(?&lt;ITEM1&gt;[#/@%0-9a-z\^ '\.\-]{1,})","(?&lt;ITEM2&gt;[#/@%0-9a-z\^ '\.\-]{1,})"(,"(?&lt;ITEMX&gt;[#/@%0-9a-z\^ '\.\-]{1,}(\([#/@%0-9a-z\^ '\.\-]{1,}\))+)"){1,}\r</Expression>          <HeaderItem>            <HeaderName>ITEM1</HeaderName>            <Value>(?&lt;ITEM1&gt;[#/@%0-9a-z\^ '\.\-]{1,})</Value>          </HeaderItem>          <HeaderItem>            <HeaderName>ITEM2</HeaderName>            <Value>(?&lt;ITEM2&gt;[#/@%0-9a-z\^ '\.\-]{1,})</Value>          </HeaderItem>          <HeaderItem>            <HeaderName>ITEMX</HeaderName>            <Value>(?&lt;ITEMX&gt;[#/@%0-9a-z\^ '\.\-]{1,})(\([#/@%0-9a-z\^ '\.\-]{1,}\))?</Value>            <Unit>\((?&lt;ITEMX&gt;[#/@%0-9a-z\^ '\.\-]{1,})\)</Unit>          </HeaderItem>        </Header>        <Row>          <Expression>"(?&lt;ITEM1&gt;[#/@%0-9\^ '\.\-]{1,})","(?&lt;ITEM2&gt;[\s#/@%0-9a-z\^ '\.\:_!\-]{1,})"!{,(?&lt;ITEMX&gt;[#/%e0-9 \.\-]{1,})!}\r\s*</Expression>          <RowIdentity>(?&lt;ITEM1&gt;[#/@%0-9\^ '\.\-]{1,})</RowIdentity>        </Row>      </Table>      <Table name="Summary Two">        <FileCode>FileCode1</FileCode>        <Header>          <Expression>\r\s*"(?&lt;ITEM1&gt;[#/@%0-9a-z\^ '\.\-]{1,})"(,"\s*(?&lt;ITEMX&gt;[#/@%0-9a-z\^ '\.\-\(\)]{1,})"," "){2,}\r</Expression>          <HeaderItem>            <HeaderName>ITEM1</HeaderName>            <Value>[#/@%0-9a-z\^ '\.\-\(\)]{1,}</Value>          </HeaderItem>          <HeaderItem>            <HeaderName>ITEMX</HeaderName>            <Value>(?&lt;ITEMX&gt;[#/@%0-9a-z\^ '\.\-]{1,})(\([#/@%0-9a-z\^ '\.\-]{1,}\))?</Value>            <Unit>\((?&lt;ITEMX&gt;[#/@%0-9a-z\^ '\.\-]{1,})\)</Unit>          </HeaderItem>        </Header>        <Row>          <Expression>"(?&lt;ITEM1&gt;[#/@%0-9\^ '\.\-]{1,})"!{,(?&lt;ITEMX&gt;[#/%e0-9\.\"\-]{1,})," "!}\r\s*</Expression>          <RowIdentity>(?&lt;ITEM1&gt;[#/@%0-9\^ '\.\-]{1,})</RowIdentity>        </Row>      </Table>    </TextReaders>    <DataBuilders>      <Definition name="Gauge Area">        <DefinitionProperty>          <Modifier>divide</Modifier>          <Modifier>power</Modifier>          <Modifier>multiply</Modifier>          <Parameter>Gauge Outer Diameter</Parameter>          <Parameter>2</Parameter>          <Parameter>2</Parameter>          <Parameter>3.14159265358897932</Parameter>        </DefinitionProperty>        <DefinitionProperty>          <Modifier>multiply</Modifier>          <Parameter>Gauge Thickness</Parameter>          <Parameter>Gauge Width</Parameter>        </DefinitionProperty>        <Unit>in^2</Unit>      </Definition>      <Definition name="Strain">        <DefinitionProperty>          <Modifier>divide</Modifier>          <Modifier>multiply</Modifier>          <Parameter>Displacement</Parameter>          <Parameter>Strain Transducer 1 Length</Parameter>          <Parameter>100</Parameter>        </DefinitionProperty>        <Unit>% strain</Unit>      </Definition>      <Definition name="Stress">        <DefinitionProperty>          <Modifier>divide</Modifier>          <Parameter>Test Load</Parameter>          <Parameter>Gauge Area</Parameter>        </DefinitionProperty>        <Unit>psi</Unit>      </Definition>      <Functional name="Tensile Response (11 axis)">        <FunctionalType>Series</FunctionalType>        <FileCode>FileCode0</FileCode>        <YAxis>Stress</YAxis>        <XAxis name="Strain">Strain</XAxis>        <Interpolate>true</Interpolate>        <LineType>lines</LineType>      </Functional>      <TableReader name="Ultimate Tensile Strength">        <RowIndex>SPECIMENCOUNT</RowIndex>        <TableName>Summary Two</TableName>        <ColumnHeaders>          <ColumnHeader>Stress at Max.Load</ColumnHeader>          <ColumnHeader>Ultimate Tensile Strength</ColumnHeader>          <ColumnHeader>Ultimate Stress</ColumnHeader>        </ColumnHeaders>      </TableReader>      <TableReader name="Young's Modulus (11-axis)">        <RowIndex>SPECIMENCOUNT</RowIndex>        <TableName>Summary Two</TableName>        <ColumnHeaders>          <ColumnHeader>Modulus</ColumnHeader>          <ColumnHeader>Young's Modulus</ColumnHeader>        </ColumnHeaders>      </TableReader>      <TableReader name="0.2% Offset Yield Stress">        <RowIndex>SPECIMENCOUNT</RowIndex>        <TableName>Summary Two</TableName>        <ColumnHeaders>          <ColumnHeader>Stress at offset Yield 1</ColumnHeader>          <ColumnHeader>0.2% Yield Strength</ColumnHeader>          <ColumnHeader>Yield Stress</ColumnHeader>        </ColumnHeaders>      </TableReader>      <TableReader name="Reduction in Area">        <RowIndex>SPECIMENCOUNT</RowIndex>        <TableName>Summary Two</TableName>        <ColumnHeaders>          <ColumnHeader>% Red. Area</ColumnHeader>        </ColumnHeaders>      </TableReader>      <TableReader name="Strain at Tensile Failure">        <RowIndex>SPECIMENCOUNT</RowIndex>        <TableName>Summary Two</TableName>        <ColumnHeaders>          <ColumnHeader>Strain Total Elongation</ColumnHeader>          <ColumnHeader>Total %Strain @ Break</ColumnHeader>          <ColumnHeader>% Strain at Auto. Break</ColumnHeader>        </ColumnHeaders>      </TableReader>      <Text name="Additional Information">        <Item>Tensile Comments</Item>      </Text>      <Text name="Additional Test Notes">        <Item>UDF Index</Item>        <Item>TEXT:: </Item>        <Item>UDF Label</Item>        <Item>TEXT: : </Item>        <Item>UDF Comment</Item>      </Text>      <Text name="ThisFileName0+ext">        <Item>ThisFileName0</Item>        <Item>TXT</Item>      </Text>      <Text name="ThisFileName1+ext">        <Item>ThisFileName1</Item>        <Item>RLT</Item>      </Text>      <Text name="TreeTemp">        <Item>Test Temperature</Item>        <Item>TempUnit</Item>      </Text>      <FileReader name="Test File 1">        <FileName>ThisFilePath0</FileName>        <Description>ThisFileName0+ext</Description>        <AllowIndex>false</AllowIndex>        <DisplayAsRecord>false</DisplayAsRecord>        <Target>NEW_WINDOW</Target>      </FileReader>      <FileReader name="Test File 2">        <FileName>ThisFilePath1</FileName>        <Description>ThisFileName1+ext</Description>        <AllowIndex>false</AllowIndex>        <DisplayAsRecord>false</DisplayAsRecord>        <Target>NEW_WINDOW</Target>      </FileReader>    </DataBuilders>    <TemplateProperties>      <TemplateProperty name="SPECIMENCOUNT">        <Property>Specimen</Property>      </TemplateProperty>      <TemplateProperty name="TEXT: : ">        <Property>Text</Property>        <Value> : </Value>      </TemplateProperty>      <TemplateProperty name="TEXT:: ">        <Property>Text</Property>        <Value>: </Value>      </TemplateProperty>      <TemplateProperty name="ThisFilePath0">        <Property>FilePath</Property>        <FileCode>FileCode0</FileCode>      </TemplateProperty>      <TemplateProperty name="ThisFileName0">        <Property>FileName</Property>        <FileCode>FileCode0</FileCode>      </TemplateProperty>      <TemplateProperty name="ThisFilePath1">        <Property>FilePath</Property>        <FileCode>FileCode1</FileCode>      </TemplateProperty>      <TemplateProperty name="ThisFileName1">        <Property>FileName</Property>        <FileCode>FileCode1</FileCode>      </TemplateProperty>      <TemplateProperty name="RLT">        <Property>Text</Property>        <Value>.rlt</Value>      </TemplateProperty>      <TemplateProperty name="TXT">        <Property>Text</Property>        <Value>.txt</Value>      </TemplateProperty>      <TemplateProperty name="Polymers">        <Property>Text</Property>        <Value>Polymers</Value>      </TemplateProperty>      <TemplateProperty name="TempUnit">        <Property>Text</Property>        <Value> °F</Value>      </TemplateProperty>    </TemplateProperties>  </Data></Template>